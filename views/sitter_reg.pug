form(action='/contact' method='POST')
  .form-row
    .form-group.col-md-6
      label(for='inputEmail4') Email
      input#inputEmail4.form-control(type='text' placeholder='Email' name='email')
    .form-group.col-md-6
      label(for='inputPassword4') Password
      input#inputPassword4.form-control(type='text' placeholder='Password' name='password')
  .form-group
    label(for='inputAddress') Address
    input#inputAddress.form-control(type='text' placeholder='1234 Main St' name='addressOne')
  .form-group
    label(for='inputAddress2') Apartment
    input#inputAddress2.form-control(type='text' placeholder='Apartment, studio, or floor' name='apartment')
  .form-row
    .form-group.col-md-6
      label(for='inputCity') City
      input#inputCity.form-control(type='text' name='city')
    .form-group.col-md-4
      label(for='inputState') State
      select#inputState.form-control(name='state')
        option(selected='') Gulu
        option Lira
        option Arua
    .form-group.col-md-2
      label(for='inputZip') Zip
      input#inputZip.form-control(type='text')
  .form-group
    .form-check
      input#gridCheck.form-check-input(type='checkbox')
      label.form-check-label(for='gridCheck')  Check me out 
  button.btn.btn-primary(type='submit') Sign in
